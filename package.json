{
  "name": "waikato-drilling",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:prod": "run-p type-check \"build-only:prod {@}\" --",
    "build-only": "vite-ssg build --mode=staging",
    "build-only:prod": "vite-ssg build --mode=prod",
    "deploy": "npm run build && gh-pages -d dist",
    "deploy:prod": "npm run build:prod && gh-pages -d dist",
    "type-check": "vue-tsc --build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueuse/head": "^2.0.0",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@unhead/vue": "^2.0.14",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "gh-pages": "^6.3.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vite-ssg": "^28.1.0",
    "vite-ssg-sitemap": "^0.10.0",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.4"
  }
}
